<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F4F8"
    android:orientation="vertical"
    tools:context=".activitys.SellConform">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragments.OrderConformFragment">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/act_s_c_foodphoto"
                android:layout_width="match_parent"
                android:layout_height="230dp"
                android:background="@color/light_lemon"
                android:contentDescription="@string/tiffin_image"
                android:elevation="35dp"
                android:scaleType="fitXY"
                android:src="@drawable/loading_image" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraint_layout_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-30dp"
                android:layout_marginBottom="-5dp"
                android:background="@drawable/order_conform_card_background"
                android:elevation="35dp"
                android:outlineAmbientShadowColor="@color/dark_green"
                android:outlineSpotShadowColor="@color/dark_green"
                tools:targetApi="p">

                <TextView
                    android:id="@+id/scfoodname"
                    android:layout_width="wrap_content"
                    android:layout_height="60sp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:fontFamily="@font/copse_regular"
                    android:gravity="center"
                    android:text="@string/please_wait"
                    tools:text="Gujarati Thaali"
                    android:textColor="@color/dark_red"
                    android:textSize="25sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/scfoodvalue"
                    android:layout_width="wrap_content"
                    android:layout_height="60sp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:fontFamily="@font/copse_regular"
                    android:gravity="center"
                    android:text="@string/please_wait"
                    android:textColor="@color/orange"
                    android:textSize="25sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="@+id/scfoodname"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="₹500" />

                <View
                    android:id="@+id/view2"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="20dp"
                    android:background="@color/mid_light_grey"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/scfoodname" />

                <LinearLayout
                    android:id="@+id/linearLayout3"
                    android:layout_width="match_parent"
                    android:layout_height="50sp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/view2">

                    <ImageView
                        android:id="@+id/nonvegveg"
                        android:layout_width="0dp"
                        android:layout_height="30dp"
                        android:layout_gravity="center"
                        android:layout_weight="2"
                        android:contentDescription="@string/vegNonVeg"
                        android:src="@drawable/veg_symbol" />

                    <TextView
                        android:id="@+id/scfoodweight"
                        android:layout_width="0dp"
                        android:layout_height="50sp"
                        android:layout_weight="4"
                        android:fontFamily="@font/fredoka_one_regular"
                        android:gravity="center"
                        android:text="@string/please_wait"
                        android:textColor="@color/dark_blue_1"
                        android:textSize="19sp"
                        tools:text="500 grams" />


                    <ImageView
                        android:id="@+id/act_sc_sharetiffinlink"
                        android:layout_width="0dp"
                        android:layout_height="25sp"
                        android:layout_gravity="center"
                        android:layout_weight="2"
                        android:contentDescription="@string/tiffin_link"
                        android:src="@drawable/share_tiffin_link" />

                </LinearLayout>

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="20dp"
                    android:background="@color/mid_light_grey"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />


                <TextView
                    android:id="@+id/scdescription"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="15dp"
                    android:fontFamily="@font/copse_regular"
                    android:gravity="center_vertical|start"
                    android:justificationMode="inter_word"
                    android:text="@string/tiffin_des"
                    android:textColor="@color/dark_green"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/view3"
                    tools:text="This tiffin contains 1 Batata Sabji, 1 Punjabi Sabji, 5 Butter Roti, 1 Masala Chaas, 1 Daal Rice" />

                <LinearLayout
                    android:id="@+id/linearLayout4"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_marginStart="17dp"
                    android:layout_marginTop="32dp"
                    android:layout_marginEnd="17dp"
                    android:orientation="vertical"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/scdescription">


                    <TextView
                        android:id="@+id/scfoodleft"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:layout_marginEnd="5dp"
                        android:fontFamily="@font/fredoka_one_regular"
                        android:gravity="center_vertical|end"
                        android:text="@string/please_wait"
                        android:textColor="@color/dark_yellow"
                        android:textSize="25sp"
                        tools:text="100 Orders left" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout6"
                    android:layout_width="match_parent"
                    android:layout_height="55dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="15dp"
                    android:orientation="horizontal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

                    <Button
                        android:id="@+id/scordertime"
                        android:layout_width="0dp"
                        android:layout_height="55sp"
                        android:layout_marginStart="15dp"
                        android:layout_marginEnd="15dp"
                        android:layout_weight="1"
                        android:background="@drawable/add_instruction_background"
                        android:fontFamily="@font/montserrat_bold"
                        android:gravity="center"
                        android:text="@string/add_order_time"
                        android:textColor="@color/orange_1"
                        android:textSize="14sp"
                        style="?android:attr/buttonBarButtonStyle" />


                    <Button
                        android:id="@+id/act_sc_location"
                        android:layout_width="0dp"
                        android:layout_height="55sp"
                        android:layout_marginStart="15dp"
                        android:layout_marginEnd="15dp"
                        android:layout_weight="1"
                        android:background="@drawable/add_instruction_background"
                        android:fontFamily="@font/montserrat_bold"
                        android:gravity="center"
                        android:text="Add Location"
                        android:textColor="@color/orange_1"
                        android:textSize="14sp"
                        style="?android:attr/buttonBarButtonStyle" />


                </LinearLayout>


                <LinearLayout
                    android:id="@+id/linearLayout5"
                    android:layout_width="match_parent"
                    android:layout_height="60sp"
                    android:layout_marginTop="30dp"
                    android:layout_marginBottom="20dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout6">

                    <LinearLayout
                        android:layout_width="150dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="15dp"
                        android:background="@drawable/increment_decrement_background"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/scminus"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="3dp"
                            android:layout_marginTop="-2dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/fredoka_one_regular"
                            android:gravity="center"
                            android:paddingBottom="10dp"
                            android:text="@string/decrement"
                            android:textColor="@color/danger"
                            android:textSize="45sp" />

                        <TextView
                            android:id="@+id/scorder"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:fontFamily="@font/fredoka_one_regular"
                            android:gravity="center"
                            android:text="@string/currentOrder"
                            android:textColor="@color/dark_blue_1"
                            android:textSize="29sp" />

                        <TextView
                            android:id="@+id/scplus"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="3dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/fredoka_one_regular"
                            android:gravity="center"
                            android:paddingBottom="8dp"
                            android:text="@string/increment"
                            android:textColor="@color/green"
                            android:textSize="45sp" />

                    </LinearLayout>

                    <Button
                        android:id="@+id/scconformbutton"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginStart="10dp"
                        android:layout_marginEnd="16dp"
                        android:background="@drawable/conform_button_background"
                        android:fontFamily="@font/montserrat_bold"
                        android:gravity="center"
                        android:paddingStart="5dp"
                        android:paddingEnd="5dp"
                        android:text="@string/checkout"
                        android:textColor="@color/white"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="@id/scconformbutton"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textInputLayout"
                        app:layout_constraintVertical_bias="0.75"
                        tools:text="checkout     ₹5000" />

                </LinearLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>


    </ScrollView>

